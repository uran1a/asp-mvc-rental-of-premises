@model ICollection<RentalOfPremises.Models.User>
@{
}
<div class="body-wrapper container-fluid p-0">
    <!-- content area -->
    <div class="content-wrapper" id="content">
        <div class="doc-hero-wrap">
            <div class="hero-wrap-inner">
                <h2 class="title">Админка</h2>
            </div>
        </div>
            <!-- Getting start -->
        <div class="content-section" id="users" style="display: block;">
            <div class="table_page table-responsive">
            <table>
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Логин</th>
                        <th>Пароль</th>
                        <th>ФИО</th>
                        <th>Серия паспорта</th>
                        <th>Код паспорта</th>
                        @*<th>Страница</th>*@
                        <th>Изменение</th>
                        <th>Удаление</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var user in @Model)
                    {   
                        <tr>
                            <td style="min-width: 0;">@user.Id</td>
                            <td style="min-width: 0;">@user.Login</td>
                            <td style="min-width: 0;">@user.Password</td>
                            <td style="min-width: 0;">@user.PhysicalEntity.Surname @user.PhysicalEntity.Name @user.PhysicalEntity.Patronymic</td>
                            <td style="min-width: 0;">@user.PhysicalEntity.Passport_Serial</td>
                            <td style="min-width: 0;">@user.PhysicalEntity.Passport_Code</td>
                            @*<td ><a asp-action="Index" asp-controller="Placement" asp-route-id="@user.Id"><img src="~/images/icons/browser.png" alt=""></a></td>*@
                            <td style="min-width: 0;"><a asp-action="UpdateUser" asp-controller="Admin" asp-route-id="@user.Id"><img src="~/images/icons/edit.png" alt=""></a></td>
                            <td style="min-width: 0;"><a asp-action="DeleteUser" asp-controller="Placement" asp-route-id="@user.Id"><img src="~/images/icons/trash_16.png" alt=""></a></td>
                        </tr>
                    }
                </tbody>
            </table>
            </div>
        </div>
    </div>
</div>
