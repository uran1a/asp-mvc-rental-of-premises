<!DOCTYPE HTML>
<html>
	<head>
		@await Html.PartialAsync("MetatagsPartial")
		@await Html.PartialAsync("CssPartial")
		@await Html.PartialAsync("AdminCssPartial")
	</head>
	<body>
		
		<!-- Main -->
		<div class="container-fluid" style="padding: 0; margin: 0;">
			<div class="row">
				<div class="col-sm-12 col-md-3 col-lg-3">
					<div class="dashboard_tab_button">
						@await Html.PartialAsync("SidebarPartial")
					</div>
				</div>
				<div class="col-sm-12 col-md-9 col-lg-9 mb-50">
					@RenderBody()
				</div>
			</div>
		</div>
		<!-- Scripts -->
		@await Html.PartialAsync("ScriptsPartial")
		@await Html.PartialAsync("AdminScriptsPartial")
	</body>
</html>